<html>
<head>
    <% include common/head %>
    <!--additional JS and css-->
    <script src="/js/app/pages/admin.js" type="text/javascript"></script>
</head>
<body>
<header id="header">
    <% include common/header %>
</header>

<section id="content_body">
    <div class="space-box">
        <div class="container-fluid">
            <form class="defaultform row">
                <div class="col-md-6 col-md-offset-3">
                    <div class="register-widget clearfix">
                        <div class="space-top-small"></div>
                        <div class="widget-title">
                            <h2>Admin Login</h2>
                            <div id="errorMsg">

                            </div>
                        </div><!-- end title -->
                        <div class="row">
                            <div class="form-group col-lg-12">
                                <label>Username or Email</label>
                                <input type="text" id="username"  placeholder="Enter email address" class="form-control">
                            </div>
                            <div class="form-group col-lg-12">
                                <label>Password</label>
                                <input type="password" id="password" placeholder="Enter password" class="form-control">
                            </div>
                            <div class="col-sm-12">
                                <button type="button" id="btnLogin" class="btn btn-default">Login</button><br>
                            </div>
                        </div>
                    </div>
                </div>
            </form><!-- end row -->
        </div><!-- end container -->
    </div>

</section>


<footer id="footer">
    <% include common/footer %>
</footer>

</body>
</html>
